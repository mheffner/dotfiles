# Include HAML files
--type-set
haml=.haml

# Include SASS files
--type-set
sass=.sass

# Include JSON files
--type-set
json=.json

# Include coffee files (XXX: as JS for now)
--type-set
coffee=.coffee

# Feature files
--type-set
feature=.feature

# Handlebar JS files
--type-set
handlebars=.handlebars

# Ignore bundler directory
--ignore-dir=.bundle
